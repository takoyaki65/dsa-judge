{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string"
      },
      "compileCommand": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "mainSourceFile": {
        "type": "string"
      },
      "associatedFiles": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "runCommand": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "testCases": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "inputFile": {
              "type": [
                "string",
                "null"
              ]
            },
            "outputFile": {
              "type": [
                "string",
                "null"
              ]
            },
            "stderrFile": {
              "type": [
                "string",
                "null"
              ]
            }
          },
          "required": [
            "inputFile",
            "outputFile",
            "stderrFile"
          ]
        }
      },
      "timeOut": {
        "type": "integer"
      },
      "memoryLimit": {
        "type": "integer"
      },
      "memoryLeakCheck": {
        "type": "boolean"
      }
    },
    "required": [
      "name",
      "compileCommand",
      "mainSourceFile",
      "runCommand",
      "testCases",
      "timeOut",
      "memoryLimit",
      "memoryLeakCheck"
    ],
    "additionalProperties": false
  }
}